#include <stdio.h>

int main()
{
    int a = -1, i = 1;
    printf("Please, enter the integer that is bigger than 0.\n");
    while (1)
    {
        scanf("%d", &a);
        if (a > 0) break;
        else printf("You were not supposed to do that. Please, input a normal value.\n");
    }
    
    while(1)
    {
        if (i * i <= a) printf("%d ", i * i);
        else break;
        i++;
    }
}
