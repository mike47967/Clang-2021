#include <stdio.h>

int main()
{
    double pi = 3.14159265;
    printf("The π with 2 symbols     The π with 4 symbols     The π with 8 symbols\n        is                        is                        is\n\n       %0.2lf                     %0.4lf                   %0.6lf", pi, pi, pi) ;

    return 0;
}
