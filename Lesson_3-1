#include <stdio.h>
 
int main() 
{
    unsigned long long a, x = 10401309014; // буквенный пароль, зашифрованный шифром A1Z26 и разделённый нулями
    printf("Welcome to our super secret database access point!\nPlease, enter password:   ");
    scanf("%llu", &a);
    printf("%s", a == x ? "Access granted" : "Access denied");
}
